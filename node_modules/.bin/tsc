#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/python/Downloads/git_download/algorithm-in-go-and-c/node_modules/typescript/bin/node_modules:/Users/python/Downloads/git_download/algorithm-in-go-and-c/node_modules/typescript/node_modules:/Users/python/Downloads/git_download/algorithm-in-go-and-c/node_modules:/Users/python/Downloads/git_download/node_modules:/Users/python/Downloads/node_modules:/Users/python/node_modules:/Users/node_modules:/node_modules:/Users/python/Downloads/git_download/algorithm-in-go-and-c/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/python/Downloads/git_download/algorithm-in-go-and-c/node_modules/typescript/bin/node_modules:/Users/python/Downloads/git_download/algorithm-in-go-and-c/node_modules/typescript/node_modules:/Users/python/Downloads/git_download/algorithm-in-go-and-c/node_modules:/Users/python/Downloads/git_download/node_modules:/Users/python/Downloads/node_modules:/Users/python/node_modules:/Users/node_modules:/node_modules:/Users/python/Downloads/git_download/algorithm-in-go-and-c/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../typescript/bin/tsc" "$@"
fi
